    "ts-jest": "^28.0.2"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "jest --bail --passWithNoTests --findRelatedTests"
    ]
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.18"
  }
