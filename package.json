{
  "name": "granlund.io",
  "version": "1.0.0",
  "private": true,
  "description": "granlund.io",
  "author": "Joatin Granlund",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
